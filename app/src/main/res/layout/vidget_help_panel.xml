<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="#000000"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="50dp" />

    <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
        android:id="@+id/ring_1"
        style="@style/Ring"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_1"
        style="@style/RingTextView"
        android:text="1"
        app:layout_constraintBottom_toBottomOf="@+id/ring_1"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ring_1" />

    <TextView
        android:id="@+id/text_1_description"
        style="@style/DescribeTextView"
        android:text="Api key"
        app:layout_constraintBottom_toBottomOf="@+id/ring_1"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/ring_1" />

    <View
        android:id="@+id/view_bridge_1"
        style="@style/Bridge"
        app:layout_constraintEnd_toEndOf="@id/ring_1"
        app:layout_constraintStart_toStartOf="@id/ring_1"
        app:layout_constraintTop_toBottomOf="@id/ring_1" />


    <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
        android:id="@+id/ring_2"
        style="@style/Ring"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_bridge_1" />

    <TextView
        android:id="@+id/text_2"
        style="@style/RingTextView"
        android:text="2"
        app:layout_constraintBottom_toBottomOf="@+id/ring_2"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ring_2" />

    <TextView
        android:id="@+id/text_2_description"
        style="@style/DescribeTextView"
        android:text="Choose file"
        app:layout_constraintBottom_toBottomOf="@+id/ring_2"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/ring_2" />

    <View
        android:id="@+id/view_bridge_2"
        style="@style/Bridge"
        app:layout_constraintEnd_toEndOf="@id/ring_2"
        app:layout_constraintStart_toStartOf="@id/ring_2"
        app:layout_constraintTop_toBottomOf="@id/ring_2" />


    <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
        android:id="@+id/ring_3"
        style="@style/Ring"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_bridge_2" />

    <TextView
        android:id="@+id/text_3"
        style="@style/RingTextView"
        android:text="3"
        app:layout_constraintBottom_toBottomOf="@+id/ring_3"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ring_3" />

    <TextView
        android:id="@+id/text_3_description"
        style="@style/DescribeTextView"
        android:text="Choose file language"
        app:layout_constraintBottom_toBottomOf="@+id/ring_3"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/ring_3" />


    <View
        android:id="@+id/view_bridge_3"
        style="@style/Bridge"
        app:layout_constraintEnd_toEndOf="@id/ring_3"
        app:layout_constraintStart_toStartOf="@id/ring_3"
        app:layout_constraintTop_toBottomOf="@id/ring_3" />


    <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
        android:id="@+id/ring_4"
        style="@style/Ring"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_bridge_3" />

    <TextView
        android:id="@+id/text_4"
        style="@style/RingTextView"
        android:text="4"
        app:layout_constraintBottom_toBottomOf="@+id/ring_4"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ring_4" />

    <TextView
        android:id="@+id/text_4_description"
        style="@style/DescribeTextView"
        android:text="Choose translate languages"
        app:layout_constraintBottom_toBottomOf="@+id/ring_4"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/ring_4" />

    <View
        android:id="@+id/view_bridge_4"
        style="@style/Bridge"
        app:layout_constraintEnd_toEndOf="@id/ring_4"
        app:layout_constraintStart_toStartOf="@id/ring_4"
        app:layout_constraintTop_toBottomOf="@id/ring_4" />

    <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
        android:id="@+id/ring_5"
        style="@style/Ring"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_bridge_4" />

    <TextView
        android:id="@+id/text_5"
        style="@style/RingTextView"
        android:text="5"
        app:layout_constraintBottom_toBottomOf="@+id/ring_5"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ring_5" />

    <TextView
        android:id="@+id/text_5_description"
        style="@style/DescribeTextView"
        android:text="Translate"
        app:layout_constraintBottom_toBottomOf="@+id/ring_5"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/ring_5" />


</merge>