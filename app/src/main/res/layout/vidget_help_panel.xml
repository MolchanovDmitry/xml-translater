<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#000000"
    android:orientation="vertical"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="50dp" />

    <FrameLayout
        android:id="@+id/frame_1"
        android:layout_width="50dp"
        android:layout_height="66dp"
        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
            android:id="@+id/ring_1"
            style="@style/Ring" />

        <TextView
            android:id="@+id/text_1"
            style="@style/RingTextView"
            android:text="1" />

        <View
            android:id="@+id/view_bridge_1"
            style="@style/Bridge" />

    </FrameLayout>

    <TextView
        android:id="@+id/text_1_description"
        style="@style/DescribeTextView"
        android:text="Api key"
        app:layout_constraintBottom_toBottomOf="@+id/frame_1"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/frame_1" />

    <FrameLayout
        android:id="@+id/frame_2"
        android:layout_width="50dp"
        android:layout_height="66dp"
        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/frame_1">

        <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
            android:id="@+id/ring_2"
            style="@style/Ring" />

        <TextView
            android:id="@+id/text_2"
            style="@style/RingTextView"
            android:text="2" />

        <View
            android:id="@+id/view_bridge_2"
            style="@style/Bridge" />

    </FrameLayout>

    <TextView
        android:id="@+id/text_2_description"
        style="@style/DescribeTextView"
        android:text="Choose file"
        app:layout_constraintBottom_toBottomOf="@+id/frame_2"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/frame_2" />


    <FrameLayout
        android:id="@+id/frame_3"
        android:layout_width="50dp"
        android:layout_height="66dp"
        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/frame_2">

        <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
            android:id="@+id/ring_3"
            style="@style/Ring" />

        <TextView
            android:id="@+id/text_3"
            style="@style/RingTextView"
            android:text="3" />

        <View
            android:id="@+id/view_bridge_3"
            style="@style/Bridge" />

    </FrameLayout>

    <TextView
        android:id="@+id/text_3_description"
        style="@style/DescribeTextView"
        android:text="Choose file language"
        app:layout_constraintBottom_toBottomOf="@+id/frame_3"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/frame_3" />

    <FrameLayout
        android:id="@+id/frame_4"
        android:layout_width="50dp"
        android:layout_height="66dp"
        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/frame_3">

        <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
            android:id="@+id/ring_4"
            style="@style/Ring" />

        <TextView
            android:id="@+id/text_4"
            style="@style/RingTextView"
            android:text="4" />

        <View
            android:id="@+id/view_bridge_4"
            style="@style/Bridge" />

    </FrameLayout>

    <TextView
        android:id="@+id/text_4_description"
        style="@style/DescribeTextView"
        android:text="Choose translate languages"
        app:layout_constraintBottom_toBottomOf="@+id/frame_4"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/frame_4" />

    <FrameLayout
        android:id="@+id/frame_5"
        android:layout_width="50dp"
        android:layout_height="66dp"
        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/frame_4">

        <com.di.penopllast.xmltranslater.presentation.ui.widget.RingView
            android:id="@+id/ring_5"
            style="@style/Ring"
            app:layout_constraintEnd_toStartOf="@id/guideline"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view_bridge_4" />

        <TextView
            android:id="@+id/text_5"
            style="@style/RingTextView"
            android:text="5" />

    </FrameLayout>

    <TextView
        android:id="@+id/text_5_description"
        style="@style/DescribeTextView"
        android:text="Translate"
        app:layout_constraintBottom_toBottomOf="@+id/frame_5"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toTopOf="@+id/frame_5" />

</merge>